        "@typescript-eslint/parser": "^5.54.0",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "prettier": "^3.4.2",
        "rimraf": "^6.0.1",
        "serverless": "^3.28.1",
        "ts-loader": "^9.5.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.2"
